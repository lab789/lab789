
#include <iostream>
using namespace std;

int main() {
    int adj[100][100];
    int n, m;
    
    cout << "Nodes: ";
    cin >> n;
    cout << "Edges: ";
    cin >> m;
    
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            adj[i][j] = 0;
        }
    }
    
    cout << "Enter edges:\n";
    for (int i = 0; i < m; i++) {
        int a, b;
        cin >> a >> b;
        adj[a][b] = 1;
        adj[b][a] = 1;
    }
    
    cout << "\nAdjacency Matrix:\n";
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            cout << adj[i][j] << " ";
        }
        cout << endl;
    }
    
   return 0;
}
